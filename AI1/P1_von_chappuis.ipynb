    "    ['True', 'False'], # Alarm\n",
    "    'True',            # John calls\n",
    "    ['True', 'False'], # Mary calls\n",
    "], model) / partial_probabilities([\n",
    "    'False',           # Burglary\n",
    "    ['True', 'False'], # Earthquake\n",
    "    ['True', 'False'], # Alarm\n",
    "    ['True', 'False'], # John calls\n",
    "    ['True', 'False'], # Mary calls\n",
    "], model)\n",
    "\n",
    "task_3 = partial_probabilities([\n",
